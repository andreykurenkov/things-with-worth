	public static int runBruteForce(String t, String p) {
		for(int i=0;i<t.length()-p.length();i++){
			boolean match=true;
				for(int j=0;j<p.length();j++){
					if(p.charAt(i)!=t.charAt(j)){
						match=false;
						break;
					}
					comparision++;
				}
		}
		return 0;
	}

